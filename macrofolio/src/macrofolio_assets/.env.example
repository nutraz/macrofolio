# RevenueCat Configuration
# Get your API key from the RevenueCat Dashboard: https://app.revenuecat.com/
# For testing, use your public SDK key (starts with "public_")
VITE_REVENUECAT_API_KEY=your_public_sdk_key_here

# Demo Mode
# Set to "false" to disable demo mode and use real RevenueCat integration
VITE_DEMO_MODE=true

# Supabase Configuration
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Blockchain Configuration
VITE_BLOCKCHAIN_NETWORK=polygon_amoy
VITE_CONTRACT_ADDRESS=your_contract_address

